{
  "name": "yapi-to-code",
  "version": "0.1",
  "description": "yapi-to-code!",
  "permissions": [
    "storage"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./public/images/logo16.png",
      "32": "./public/images/logo32.png",
      "48": "./public/images/logo48.png",
      "128": "./public/images/logo48.png"
    }
  },
  "icons": {
    "16": "./public/images/logo16.png",
    "32": "./public/images/logo32.png",
    "48": "./public/images/logo48.png",
    "128": "./public/images/logo48.png"
  },
  "options_page": "./dist/options/index.html",
  "content_scripts": [
    {
      "matches": [
        "https://*/project/*/interface/api/*",
        "http://*/project/*/interface/api/*"
      ],
      "js": [
        "dist/inject.091d656dc54ae9495b1b.js"
      ],
      "css": []
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}